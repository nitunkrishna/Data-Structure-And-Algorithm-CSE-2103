#include<iostream>

using namespace std;

void tower(int n, char source, char des, char aux);

int main()
{
    int n;
    cin >> n;
    tower(n, 'A', 'B', 'C');
    return 0;
}

void tower(int n, char source, char des, char aux)
{
    if(n==1)
    {
        cout << "Moving disk 1 from " << source << " to " << des << endl;
        return;
    }
    tower(n-1, source, aux, des);
    cout << "Moving disk " << n << " from " << source << " to " << des << endl;
    tower(n-1, aux, des, source);
}
