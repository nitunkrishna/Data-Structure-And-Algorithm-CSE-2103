#include<iostream>

using namespace std;

int main()
{
    int row, col;
    cout << "Enter row and column: ";
    cin >> row >> col;
    int arr[row][col];

    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
            cin >> arr[i][j];
    }

    cout << "The Matrix: " << endl;;
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
            cout << arr[i][j] << " ";
        cout << endl;
    }

    bool sym=true;
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            if(arr[i][j]!=arr[j][i]) 
            {
                sym=false;
                break;
            }
        }
    }
    
    if(sym) cout << "Symmetric" << endl;
    else cout << "Not Symmetric" << endl;
    return 0;
}
