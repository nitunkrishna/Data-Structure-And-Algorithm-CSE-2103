#include<iostream>
#include<iomanip>

using namespace std;

int main()
{
    int r1, c1, r2, c2;
    cout << "Enter row and column for MATRIX-A: ";
    cin >> r1 >> c1;
    cout << "Enter row and column for MATRIX-B: ";
    cin >> r2 >> c2;
    int A[r1][c1];
    int B[r2][c2];
    int res[r1][c2];

    cout << "Input MATRIX-A: " << endl;
    for(int i=0; i<r1; i++)
    {
        for(int j=0; j<c1; j++)
            cin >> A[i][j];
    }

    cout << "Input MATRIX-B: " << endl;
    for(int i=0; i<r2; i++)
    {
        for(int j=0; j<c2; j++)
            cin >> B[i][j];
    }

    if(c1!=r2)
        cout << "Matrix A and B is not Multiplicable." << endl;
    else
    {
        for(int i=0; i<r1; i++)
        {
            for(int j=0; j<c2; j++)
            {
                res[i][j]=0;
                for(int k=0; k<c1; k++)
                    res[i][j]+=A[i][k]*B[k][j];
            }
        }
    }

    cout << "The Matrix: " << endl;;
    for(int i=0; i<r1; i++)
    {
        for(int j=0; j<c2; j++)
            cout << setw(5) << res[i][j] << " ";
        cout << endl;
    }
    return 0;
}
