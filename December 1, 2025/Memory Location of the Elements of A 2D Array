#include<iostream>

using namespace std;

int main()
{
    int row, col;
    cout << "Enter row and column: ";
    cin >> row >> col;
    int arr[row][col];

    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
            cin >> arr[i][j];
    }
    int i, j;
    cout << "ROW COL" << endl;
    for(i=0; i<row; i++)
    {
        for(j=0; j<col; j++)
            cout << " " << i << "   " << j << " " << "->" << (long long)&arr[i][j] << endl;
    }
    return 0;
}
